<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	
	<!-- Sequences for hr.invoice -->
        <record id="seq_hr_invoice" model="ir.sequence">
            <field name="name">Facture RH</field>
            <field name="code">hr.invoice</field>
            <field name="prefix">FA%(month)s%(year)s/</field>
